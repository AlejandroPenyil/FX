<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fxtry.Controller.Create.ClientCreateController">
   <children>
      <BorderPane layoutX="200.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="30.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" spacing="30.0">
                     <children>
                        <TextField fx:id="txtNombre" promptText="Nombre" />
                        <TextField fx:id="txtApellidos" promptText="Apellidos" />
                        <TextField fx:id="txtUsuario" promptText="Nombre de Usuario" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" spacing="30.0">
                     <children>
                        <TextField fx:id="txtCorreo" promptText="Correo" />
                        <TextField fx:id="txtContraseña" promptText="Contraseña" />
                        <TextField fx:id="txtDni" promptText="DNI" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" spacing="30.0">
                     <children>
                        <ComboBox fx:id="cmbRol" prefWidth="150.0" promptText="Rol" />
                        <TextField fx:id="txtDireccion" promptText="Dirección" />
                        <TextField fx:id="txtTelefono" promptText="Telefono" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </center>
         <bottom>
            <HBox fx:id="create" alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="30.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#create" text="Crear">
                     <HBox.margin>
                        <Insets bottom="15.0" top="15.0" />
                     </HBox.margin>
                  </Button>
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#goToClient" text="Cancelar">
                     <HBox.margin>
                        <Insets bottom="15.0" right="15.0" top="15.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </HBox>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
